<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_saturday</fullName>
    <active>true</active>
    <errorConditionFormula>MOD(  DATE_ESTIMEE_LIVTAISON__c  - DATE(1900, 1, 7), 7)= 6</errorConditionFormula>
    <errorDisplayField>DATE_ESTIMEE_LIVTAISON__c</errorDisplayField>
    <errorMessage>No parcel shipped out on Saturday, the estimated shipping day must not be Saturday</errorMessage>
</ValidationRule>
