<aura:component access="global" implements="flexipage:availableForAllPageTypes,force:hasRecordId,forceCommunity:availableForAllPageTypes" controller="cmpTravauxController">
    <aura:attribute name="sObj" type="String" />
	<aura:attribute name="field" type="String" default="Dents__c"/>
    <aura:attribute name="Dents" type="String[]"/>
    <aura:attribute name="Dents2" type="String[]"/>
    <aura:attribute name="Ligne" type="Ligne__c"/>
    <aura:attribute name="Title" type="String"/>
    <aura:attribute name="actif" type="Boolean"/> 
    
    <aura:attribute name="alloptions" type="List" default="[ { BillingCity: 'Crochet acetal' }, { BillingCity: 'Crochet flexible' }]" />
    <aura:attribute name="selectedOptions" type="List" default="[]" />
    
    <aura:attribute name="objInfo" type="string" default="Ligne__c" description="store object Info." />
    <aura:attribute name="depnedentFieldMap" type="map" description="store controller(key)--> dependent(values) picklist value as a map." />
    <aura:attribute name="isDependentDisable" type="boolean" default="false" description="use for, make disbale depandent field." />
    
	<aura:handler name="init" value="{!this}" action="{!c.init}"/>
   <aura:handler event="aura:doneRendering" action="{!c.onRender}"/>    
    <aura:attribute name="isDoneRendering" type="Boolean" default="false"/>

	<!-- ENTETE -->
	<div class="slds-page-header" style="background-color: #e5e5ee;border-style: solid;border-color:#e5e5ee;border-collapse: collapse;border-width:5px;">
		<div class="slds-grid" >
			<div class="slds-col slds-has-flexi-truncate">
				<div class="slds-media slds-no-space slds-grow">
    				<div class="slds-media__figure slds-icon forceEntityIcon" style="background-color: #FFFFFF">
    					<span class="uiImage">
    						  <!--  <apex:image id="Caducee" value="{!$Resource.Logo}" width="32" height="34"/>  --> 
    						  <img src="{!$Resource.Logo}" width="32" height="34"/>
    					</span> 
    				</div>
					<div class="slds-media__body">
						<nav>
							<ol class="slds-breadcrumb slds-line-height_reset">
								<li class="slds-breadcrumb__item">
									<span>Ligne</span>
								</li>
							</ol>
						</nav>
						<h1 class="slds-page-header__title slds-m-right_small slds-align-middle slds-truncate" title="Prothèse fixe">{!v.Ligne.RecordType.Name}</h1>
					</div>
				</div>
			</div>
			<div class="slds-col slds-no-flex slds-grid slds-align-top">
				<div class="slds-button-group" role="group">
					<button class="slds-button slds-button_neutral" onclick="{!c.saveLigne}">Enregistrer</button>
					<button class="slds-button slds-button_neutral" onclick="{!c.cancelLigne}">Annuler les modifications</button>
					<button class="slds-button slds-button_neutral" onclick="{!c.deleteLigne}">Supprimer la ligne</button>
				</div>
			</div>
		</div>
		<ul class="slds-grid slds-page-header__detail-row">
			<li class="slds-page-header__detail-block">
				<p class="slds-text-title slds-truncate slds-m-bottom_xx-small" title="Field 1">Ordonnance</p>
				<p class="slds-text-body_regular slds-truncate" title="Description that demonstrates truncation with a long text field">{!v.Ligne.Ordonnance__r.Name}</p>
			</li>
			<li class="slds-page-header__detail-block">
				<p class="slds-text-title slds-truncate slds-m-bottom_xx-small" title="Field 4">Ligne</p>
				<p title="Description (2-line truncation—must use JS to truncate).">{!v.Ligne.Name}</p>
			</li>
			<li class="slds-page-header__detail-block">
                <aura:if isTrue="{!v.Ligne.RecordType.Description!=null}"> 
                	<p class="slds-text-body_large" >
                    	<span class="uiImage"><img src="{!$Resource.WarningOrange}" width="32" height="34"/></span>
                    	<strong>&nbsp;&nbsp;{!v.Ligne.RecordType.Description}</strong>
					</p>
                </aura:if>
			</li>
		</ul>
	</div>	

